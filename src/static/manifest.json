{
  "name": "Instagram Likes Deleter",
  "description": "Delete all your Instagram likes with one click!",
  "version": "1.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Extension",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/your_activity/interactions/likes*"
      ],
      "js": ["contentScript.js"]
    }
  ]
}
